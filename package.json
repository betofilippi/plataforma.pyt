{
  "name": "plataforma-app",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:forever": "dev-forever.bat",
    "dev:watch": "nodemon --exec \"npm run dev\" --watch . --ext ts,tsx,js,jsx --ignore node_modules",
    "build": "vite build",
    "build:client": "vite build",
    "build:server": "echo 'Server build skipped'",
    "build:packages": "echo 'No packages to build'",
    "build:modules": "echo 'No modules to build'",
    "build:module": "node scripts/build-modules.js",
    "start": "node dist/server/node-build.mjs",
    "format.fix": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "sync:docs": "node sync-claude-docs.cjs",
    "sync:docs:watch": "node sync-claude-docs.cjs --watch",
    "typecheck": "tsc --noEmit",
    "test": "echo 'No tests configured'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testMatch='**/*.integration.(test|spec).(ts|tsx|js)'",
    "test:unit": "jest --testMatch='**/*.unit.(test|spec).(ts|tsx|js)' --testPathIgnorePatterns='**/*.integration.(test|spec).(ts|tsx|js)'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:smoke": "playwright test e2e/smoke.spec.ts",
    "test:pre-push": "npm run test:unit && npm run test:integration",
    "test:all": "./.github/scripts/test-all.sh",
    "db:migrate": "node database/migrate.cjs",
    "db:setup": "npm run db:migrate",
    "db:test": "node -e \"require('./server/database/config.js').initializeDatabase().then(() => console.log('✅ DB conectado')).catch(err => console.error('❌ DB erro:', err.message));\"",
    "cache:clear": "node -e \"const {redis} = require('./server/database/config.js'); redis.connect().then(() => redis.flushAll()).then(() => console.log('✅ Cache limpo')).catch(console.error);\"",
    "ai:setup": "node scripts/setup-ai-schema.cjs",
    "ai:test": "node scripts/test-ai-api.cjs",
    "storage:create-buckets": "node scripts/create-storage-buckets.js",
    "storage:test": "node scripts/test-document-upload.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:dev": "docker-compose --profile dev up",
    "docker:test": "docker-compose --profile test up --abort-on-container-exit",
    "docker:down": "docker-compose down",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deploy": "./.github/scripts/deploy.sh",
    "deploy:staging": "./.github/scripts/deploy.sh staging",
    "deploy:production": "./.github/scripts/deploy.sh production",
    "release": "semantic-release",
    "prepare": "husky install"
  },
