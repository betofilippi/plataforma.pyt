    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "semver": "^7.5.4",
    "tar": "^6.2.0",
    "node-cron": "^3.0.2",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "mime-types": "^2.1.35",
    "archiver": "^6.0.1",
    "fs-extra": "^11.2.0",
