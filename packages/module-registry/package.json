    "@types/mime-types": "^2.1.2",
    "@types/archiver": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.5.0",
    "typescript": "^5.5.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "jest": "^29.6.0",
    "@types/jest": "^29.5.4"
